{
	"schemaVersion": 1,
	"id": "weaponry",
	"version": "${version}",
	"name": "Weaponry",
	"description": "A small mod that adds a few new types of weapons to the game.",
	"authors": [
		"HyperionDash"
	],
	"contact": {
		"homepage": "https://modrinth.com/project/TTvWohso",
		"sources": "https://github.com/HyperionDash/Weaponry",
		"issues": "https://github.com/HyperionDash/Weaponry/issues"
	},
	"license": "MIT",
	"icon": "assets/weaponry/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.hyper.weaponry.Weaponry"
		],
		"fabric-datagen": [
			"net.hyper.weaponry.WeaponryDataGenerator"
		],
		"client": [
			"net.hyper.weaponry.WeaponryClient"
		]
	},
	"mixins": [
		"weaponry.mixins.json",
		{
			"config": "weaponry.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	}
}